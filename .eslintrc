{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "indent": 1,
    "no-use-before-define": 0,
    "curly": 2,
    "no-unused-vars": 1,
    "handle-callback-err": 1,
    "no-console": 0,
    "no-trailing-spaces": 2,
    "strict": 2,
    "no-buffer-constructor": 1
  }
}
